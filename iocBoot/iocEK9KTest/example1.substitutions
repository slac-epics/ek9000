# For TestTerm1 
# Note that the string substituted for TERMINAL is identical to the record base name
# specified in the ioc init script (in the line ek9000ConfigureTerminal("EK9K1", "TestTerm1", "EL2008", 1))
file "../../db/EL2008.template"
{ 	pattern 
	{
		TERMINAL,
		DESC1,	ZNAM1,	ONAM1,	PINI1,	
		DESC2,	ZNAM2,	ONAM2,	PINI2,	
		DESC3,	ZNAM3,	ONAM3,	PINI3,	
		DESC4,	ZNAM4,	ONAM4,	PINI4,	
		DESC5,	ZNAM5,	ONAM5,	PINI5,	
		DESC6,	ZNAM6,	ONAM6,	PINI6,	
		DESC7,	ZNAM7,	ONAM7,	PINI7,	
		DESC8,	ZNAM8,	ONAM8,	PINI8	
	}
	{
		"TestTerm1",
		"Sig1", "",		"",		"1",
		"Sig2", "",		"",		"1",
		"Sig3", "",		"",		"1",
		"Sig4", "",		"",		"1",
		"Sig5", "",		"",		"1",
		"Sig6", "",		"",		"1",
		"Sig7", "",		"",		"1",
		"Sig8", "",		"",		"1"
	}
}

