		DESC4,	ZNAM4,	ONAM4,	ZSV4,	OSV4,	COSV4,	SCAN4
	}
	{
		"TestTerm2",
		"",		"OFF",	"ON",	"0",	"0",	"0",		".1 second",
		"",		"OFF",	"ON",	"0",	"0",	"0",		".1 second",
		"",		"OFF",	"ON",	"0",	"0",	"0",		".1 second",
		"",		"OFF",	"ON",	"0",	"0",	"0",		".1 second"
	}
}

